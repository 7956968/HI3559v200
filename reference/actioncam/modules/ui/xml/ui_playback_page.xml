<?xml version="1.0" encoding="UTF-8"?>
<view
    id="PLAYBACK_PAGE"
    isusecommonskin="yes">

    <window
        id="PLAYBACK_PAGE"
        top="0"
        width="320"
        height="240"
        opacity="255"
        winlevel="0"
        transparent="yes"
        normalskin="SKIN_COMMON_BG"
        isrelease="yes"
        onshow="PDT_UI_PLAYBACK_OnShow"
        onhide="PDT_UI_PLAYBACK_OnHide"
        ontimer="PDT_UI_PLAYBACK_OnTimer"
        onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
        ongesturetap="PDT_UI_PLAYBACK_WinOnGestureTap"
        ongesturefling="PDT_UI_PLAYBACK_WinOnGestureFling"
        ongesturescroll="PDT_UI_PLAYBACK_WinOnGestureScroll"
        onevent="PDT_UI_PLAYBACK_OnEvent">

        <button
            id="PLAYBACK_BUTTON_BACK"
            top="0"
            left="0"
            width="48"
            height="48"
            isrelease="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_BACK"
            highlightskin="SKIN_PLAYBACK_BUTTON_BACK_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_BACK_ACTIVE"
            alignment="hcenter|vcenter"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <button
            id="PLAYBACK_BUTTON_VOLUME"
            top="0"
            left="48"
            width="54"
            height="48"
            isrelease="yes"
            font="FONT22"
            text=""
            normalskin="SKIN_PLAYBACK_BUTTON_SPEED"
            highlightskin="SKIN_PLAYBACK_BUTTON_SPEED_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_SPEED_ACTIVE"
            alignment="hcenter|vcenter"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <button
            id="PLAYBACK_BUTTON_INFO"
            top="0"
            left="102"
            width="124"
            height="48"
            isrelease="yes"
            font="FONT22"
            text="STRING_FILELIST_INFO"
            alignment="hcenter|vcenter"
            transparent="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_SPEED"
            highlightskin="SKIN_PLAYBACK_BUTTON_SPEED"
            activeskin="SKIN_PLAYBACK_BUTTON_SPEED"/>

        <button
            id="PLAYBACK_BUTTON_SPEED"
            top="0"
            left="226"
            width="46"
            height="48"
            isrelease="yes"
            font="FONT22"
            text="STRING_PLAYBACK_SPEED"
            alignment="hcenter|vcenter"
            transparent="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_SPEED"
            highlightskin="SKIN_PLAYBACK_BUTTON_SPEED_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_SPEED_ACTIVE"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <button
            id="PLAYBACK_BUTTON_DELETE"
            top="0"
            left="272"
            width="48"
            height="48"
            isrelease="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_DELETE"
            highlightskin="SKIN_PLAYBACK_BUTTON_DELETE_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_DELETE_ACTIVE"
            alignment="hcenter|vcenter"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <button
            id="PLAYBACK_BUTTON_PREV"
            top="96"
            left="0"
            width="48"
            height="48"
            isrelease="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_PREV"
            highlightskin="SKIN_PLAYBACK_BUTTON_PREV_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_PREV_ACTIVE"
            alignment="hcenter|vcenter"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <button
            id="PLAYBACK_BUTTON_PLAY"
            top="96"
            left="136"
            width="48"
            height="48"
            isrelease="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_PLAY"
            highlightskin="SKIN_PLAYBACK_BUTTON_PLAY_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_PLAY_ACTIVE"
            alignment="hcenter|vcenter"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <button
            id="PLAYBACK_BUTTON_NEXT"
            top="96"
            left="272"
            width="48"
            height="48"
            isrelease="yes"
            normalskin="SKIN_PLAYBACK_BUTTON_NEXT"
            highlightskin="SKIN_PLAYBACK_BUTTON_NEXT_ACTIVE"
            activeskin="SKIN_PLAYBACK_BUTTON_NEXT_ACTIVE"
            alignment="hcenter|vcenter"
            onkeydown="PDT_UI_PLAYBACK_OnKeyDown"
            ongesturetap="PDT_UI_PLAYBACK_OnGestureTap"/>

        <trackbar
            id="PLAYBACK_TRACKBAR_PLAY"
            top="180"
            left="0"
            width="320"
            height="50"
            isrelease="yes"
            normalskin="SKIN_PLAYBACK_TRACKBAR_PLAY"
            activeskin="SKIN_PLAYBACK_TRACKBAR_PLAY"
            widgetposmirror="yes"
            widgetinteriormirror="yes"
            isskinforcedraw="yes"
            normaltrack="./res/pic/playback_thumb.png"
            activetrack="./res/pic/playback_thumb.png"
            mousedowntrack="./res/pic/playback_thumb.png"
            slider="./res/pic/playback_track_slider_bg.png"
            leftmargin="12"
            rightmargin="12"
            topmargin=""
            bottommargin=""
            style="horizon"
            maxvalue="100"
            minvalue="0"
            step="1"
            ontouchaction="PDT_UI_PLAYBACK_TrackbarOnTouchAction"
            onmouseout="PDT_UI_PLAYBACK_TrackbarOnMouseOut"/>

    </window>

</view>
